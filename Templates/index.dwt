$(document).ready(function(){ 

	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 200) {
				$('#bandeau').fadeOut();
			} else {
				$('#bandeau').fadeIn();
			}
			
			if ($(this).scrollTop() > 60) {
				$('#image-bandeau').stop(true, false);
				$('#image-bandeau').animate({opacity: 0.5},400);
			} else {
				$('#image-bandeau').stop(true, false);
				$('#image-bandeau').animate({opacity: 1},400);
			}
			
		});
	});

});
</script>
